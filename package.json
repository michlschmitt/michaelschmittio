{
  "name": "@michlschmitt/portfolio-website",
  "description": "Next.js and Notion API based static portfolio website and blog.",
  "version": "1.0.3",
  "keywords": [
    "notion-api",
    "react",
    "nextjs"
  ],
  "homepage": "https://www.michaelschmitt.io",
  "bugs": {
    "url": "https://github.com/michlschmitt/portfolio-website/issues"
  },
  "license": "UNLICENSED",
  "author": "Michael Schmitt",
  "files": [
    "*"
  ],
  "main": "src/pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/michlschmitt/portfolio-website"
  },
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn generate-static-files && next build",
    "dev": "NODE_ENV=development next dev -p 3000",
    "generate-static-files": "node src/modules/generators/generate-static-files.js",
    "lint": "next lint && eslint src",
    "prepare": "husky install",
    "release": "dotenv release-it",
    "start": "next start -p 3000"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.1",
    "@notionhq/client": "^0.4.13",
    "classnames": "^2.3.1",
    "feed": "^4.2.2",
    "loader-utils": "^3.2.0",
    "next": "12.0.9",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-textarea-autosize": "^8.3.3",
    "sitemap": "^7.1.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "@svgr/webpack": "^6.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "17.0.12",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.38",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "dotenv-cli": "^4.1.1",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "release-it": "^14.12.4",
    "tailwindcss": "^3.0.17",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "engineStrict": true,
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
